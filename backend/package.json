{
  "name": "recommerce-backend",
  "version": "1.0.0",
  "description": "An online marketplace to sell, browse, and reserve pre-owned products",
  "keywords": [
    "marketplace",
    "classifieds",
    "peer-to-peer",
    "resale",
    "second-hand",
    "buy-and-sell",
    "booking-system",
    "mern",
    "rest-api",
    "mongodb",
    "express",
    "nodejs"
  ],
  "license": "MIT",
  "author": "Recommerce",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node --watch ./src/index.js",
    "start": "cross-env NODE_ENV=production node ./src/index.js",
    "build": "npm install",
    "check-format": "npx prettier --check .",
    "format": "npx prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@google/genai": "^1.42.0",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.9.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.2.1",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "nodemon": "^3.1.13",
    "prettier": "^3.8.1"
  }
}
